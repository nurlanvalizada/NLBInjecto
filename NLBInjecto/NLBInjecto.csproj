<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.0.0</Version>
        <Authors>Nurlan</Authors>
        <Company>nurlanvalizada.com</Company>
        <PackageDescription>NLBInjecto - your custom lightweight DI container.</PackageDescription>
        <PackageTags>DependencyInjection DI .NET</PackageTags>
        <RepositoryUrl>https://github.com/nurlanvalizada/NLBInjecto</RepositoryUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>
    
</Project>
